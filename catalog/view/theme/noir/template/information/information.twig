{{ header }}
<div id="information-information" class="information-contact">
	<div class="page-wrapper container">
		<h1 class="page-title cnt">{{heading_title}}</h1>
		{% if subtitle %}
		<div class="cat-short-description cnt">{{ subtitle }}</div>
		{% endif %}
		<div class="breadcrumbs df fww">
		{% for breadcrumb in breadcrumbs %}
			{% if loop.last %}
			<span>{{ breadcrumb.text }}</span>
			{% else %}
			<a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
			<i>/</i>
			{% endif %}
		{% endfor %}
		</div>
		<div id="content" class="info-page">
			{{ content_top }}
			{{ description }}
			{{ content_bottom }}
		</div>
	</div>
	{{ column_left }}
	{{ column_right }}
 </div>
{{ footer }}
</body>
</html>